<iscontent type="text/html" charset="UTF-8" compact="true" />

<!-- Custom Banner Component -->
<div class="custom-banner">
  <isif condition="${pdict.image && pdict.categoryUrl}">
    <a href="${pdict.categoryUrl}">
      <img src="${pdict.image.url}" alt="Banner Image" class="banner-image" />
    </a>
<iselseif condition="${pdict.image}" />
    <img src="${pdict.image.url}" alt="Banner Image" class="banner-image" />
  </isif>
</div>