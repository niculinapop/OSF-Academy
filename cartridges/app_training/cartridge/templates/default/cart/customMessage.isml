<!-- Custom Message Section -->
<isif condition="${!empty(pdict.customMessage)}">
  <div class="container">
    <div class="row">
      <div class="col-sm-7 col-md-8">
        <div class="custom-message"
          style="background-color: rgb(236, 184, 39); padding: 10px; margin: 20px 0; border-radius: 5px; font-size: 16px; color: #333; width: 100%; text-align: center;">
          <p style="margin: 0;">${pdict.customMessage}</p>
        </div>
      </div>
    </div>
  </div>
</isif>